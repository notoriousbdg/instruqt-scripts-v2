{
  "tags": [],
  "params": {
    "criteria": [
      {
        "comparator": ">",
        "metrics": [
          {
            "name": "A",
            "aggType": "count"
          }
        ],
        "threshold": [
          70
        ],
        "timeSize": 10,
        "timeUnit": "m"
      }
    ],
    "alertOnNoData": false,
    "alertOnGroupDisappear": false,
    "searchConfiguration": {
      "query": {
        "query": "log.file.path : \"/var/log/mysql/error.log\" ",
        "language": "kuery"
      },
      "index": "logs-*"
    }
  },
  "schedule": {
    "interval": "2m"
  },
  "consumer": "logs",
  "name": "Log Spike",
  "rule_type_id": "observability.rules.custom_threshold",
  "actions": [],
  "alert_delay": {
    "active": 1
  }
}